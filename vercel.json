{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat"
    },
    {
      "source": "/api/status",
      "destination": "/api/status"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

